{% if session.color is not null %}
  {% if session.color == "orange" %}
    <link rel="stylesheet" href="http://localhost/easy-notes/public/assets/css/override_orange.css">
  {% elseif session.color == "purple" %}
    <link rel="stylesheet" href="http://localhost/easy-notes/public/assets/css/override_purple.css">
  {% elseif session.color == "green" %}
    <link rel="stylesheet" href="http://localhost/easy-notes/public/assets/css/override_green.css">
  {% endif %}
{% endif %}
